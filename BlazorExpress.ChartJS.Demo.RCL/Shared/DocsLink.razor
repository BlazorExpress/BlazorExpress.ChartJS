@namespace BlazorExpress.ChartJS.Demo.RCL
@inherits BulmaComponentBase

<div>
    <Button Type="ButtonType.Link" Href="@Href">
        <span>@Text</span>
        <BootstrapIcon Class="ml-1" Name="BootstrapIconName.BoxArrowUpRight" />
    </Button>
</div>

@code {
    [Parameter]
    public string? Href { get; set; }

    [Parameter]
    public string? Text { get; set; } = "API Documentation";
}